<div class="container-fluid">
    <div class="container">
        <h2>{{title-web}}</h2>
        <hr>
        <div class="row">
            {{#each users}}
                    {{#if activo}}
                    <div class="col-3 p-2 mb-2">
                    {{else}}
                    <div class="col-3 p-2 mb-2 userInactivo">
                    {{/if}}
                    <div class="card p-2 mb-2">
                             <p class="m-0">{{#each nombre}} <strong>{{this}}</strong> {{/each}}</p>
                        <p class="m-0">{{localidad}}</p>
                        <p class="m-0">{{profesion}}</p>
                        {{#if activo}}
                            <p class="badge bg-success">Activo</p>
                        {{else}}
                            <p class="badge bg-danger">Baja</p>
                        {{/if}}
                    </div>
                </div>
            {{/each}}
        </div>
    </div>